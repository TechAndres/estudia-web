<!DOCTYPE html>
<html>
  <head>
    <title>EstudiaWeb</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <div class="container-fluid no-gutters" id="container-fluid-grid">
        <nav class="navbar navbar-expand-lg" id="navbar-primaria" style="background-color:#179FFF">
            <div class="row align-items-center row-nav justify-content-center" >
                  <div class="col-1-sm">
                  <a class="navbar-brand" href="/">
                    <%= image_tag("estudia-logo.png", id: "logo", alt: "logo")%></a>
                  </div>
                  <div class="col-2-sm">
                    <button type="button" class="btn btn-warning">Warning</button>
                  </div>
            </div>
        </nav>
  </div>
    <%= yield %>
  </body>
</html>
